<scxml xmlns="http://www.w3.org/2005/07/scxml"
       version="1.0"
       initial="idle"> 
    
        
   
    <state id="idle">
        <transition event="init" target="InterferometryInitializeStarted"></transition>
    </state>
    
    <parallel id="Init">
        <state id="Interferometry">
            <initial>
                <transition target="InterferometryInitializeStarted"></transition>
            </initial>
            <state id="InterferometryInitializeStarted">
                <transition event="uno" target="End">
                </transition>
            </state>
            <state id="Tres">
                
            </state>
        </state>
        
        <state id="LocalOscillator">
            <initial>
                <transition target="LocalOscillatorStarted"></transition>
            </initial>
            <state id="LocalOscillatorStarted">
                <transition event="dos" target="End"></transition>
            </state>
        </state>
        
        <state id="End">
        
    </state>
    </parallel>
</scxml>